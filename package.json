{
  "name": "bueadelights-official",
  "version": "2.0.0",
  "description": "BueaDelights - Authentic Cameroonian Cuisine Delivery | Local Flavors at Your Fingertips",
  "type": "module",
  "keywords": [
    "cameroon",
    "food-delivery",
    "buea",
    "restaurant",
    "whatsapp-ordering",
    "authentic-cuisine",
    "pwa",
    "food-business"
  ],
  "author": "Caroline Folefack Viviane <contact@bueadelights.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host",
    "type-check": "tsc --noEmit",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.295.0",
    "react-intersection-observer": "^9.5.3",
    "react-helmet-async": "^2.0.4",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
